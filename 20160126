								--g_vga <= "000";
								--b_vga <= "111";
						--elsif  (cnt_h_sync_vga > 0 and cnt_h_sync_vga < 640 and cnt_v_sync_vga = 355 )then 
								--r_vga <= "000";
								--g_vga <= "000";
								--b_vga <= "111";
