				else	
					Dilation_buf_0(SB_buf_cnt) <= Dilation_buf_1_data_9(7 downto 0);
					Dilation_buf_1(SB_buf_cnt) <= Dilation_buf_2_data_9(7 downto 0);
					Dilation_buf_2(SB_buf_cnt) <= Dilation_buf_3_data_9(7 downto 0);
