                if diff <= '1' then 

                    r_vga <= "111";
                    g_vga <= "111";
                    b_vga <= "000";
                else
                    r_vga <= buf_vga_Y(cnt_h_sync_vga)(7 downto 5);
                    g_vga <= buf_vga_Y(cnt_h_sync_vga)(7 downto 5);
